command /skillrandom:
    trigger: 
        if player has permission "sprandom.sk":
            set {rsp::%uuid of player%} to a random integer between 1 and 100000
            add {rsp::%uuid of player%} to {skills::%uuid of player%}
            set {rsp::%uuid of player%} to 0
            stop
        else:
            send "&cUnkown Command" to player

