command /voucher <text>:
    permission: *
    trigger:
        if arg-1 is "100", "200", "300", "400", "500", "600", "700", "800", "900", "1000", "2000", "3000", "4000", "5000", "6000", "7000", "8000", "9000", or "10000":
            set {_vsp} to arg 1
            give player paper named "&cSkill Point Voucher" with lore "&7Use this voucher for %{_vsp}% skill points"
            set {_vsp} to 0
        else:
            send "&cYou must use the numbers in the vouchers channel in the discord"

